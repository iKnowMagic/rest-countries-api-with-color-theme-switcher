[ignore]
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/.history/.*
<PROJECT_ROOT>/vendor/.*
<PROJECT_ROOT>/.vscode/.*

#https://github.com/facebook/flow/issues/869
[declarations]
